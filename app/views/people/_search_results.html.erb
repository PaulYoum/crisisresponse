<div class="search-results">
  <h1>Matches</h1>

  <div class="results">
    <div class="loose-matches">
      <% @people.each do |person| %>
        <%= render person, link: person_path(person) %>
      <% end %>

      <% if @people.none? %>
        <%= t("search.results.none") %>
      <% end %>
    </div>
  </div>
</div>
