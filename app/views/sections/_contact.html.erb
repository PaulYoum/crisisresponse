<section class="profile-contact">
  <div class="section-header section-header-muted">
    <div class="section-header-text">
      <span class="section-title">Contact</span>
    </div>
  </div>

  <div class="section-body">
    <div class="contact-name"><%= contact.name %></div>
    <div class="contact-relationship"><%= contact.relationship %></div>
    <div class="contact-organization"><%= contact.organization %></div>

    <% if contact.notes.present? %>
      <div class="contact-notes">
        <div class="contact-notes-label">Notes</div>
        <%= contact.notes %>
      </div>
    <% end %>
  </div>

  <%= render "section_dialer", phone: contact.cell %>
</section>
