<%= javascript_include_tag :application %>

<%= yield :javascript %>

<%= render "analytics" %>

<script src="https://maps.googleapis.com/maps/api/js?key=<%= ENV.fetch("GOOGLE_MAPS_API_KEY") %>&libraries=places&callback=initMap" async defer></script>

<% if Rails.env.test? %>
